# Configuration Supabase avec timeouts augmentés
# À ajouter dans votre .env principal

# Timeout pour les requêtes Supabase (en millisecondes)
SUPABASE_REQUEST_TIMEOUT=30000

# Timeout pour les connexions réseau
SUPABASE_CONNECT_TIMEOUT=10000

# Nombre de tentatives avant échec
SUPABASE_MAX_RETRIES=3

# Mode resilient : ignore les erreurs non-critiques
SUPABASE_RESILIENT_MODE=true
